    "express": "4.18.1",
    "mongoose": "6.4.4",
    "serve-static": "1.15.0",
    "body-parser": "1.20.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/express": "4.17.13",
    "@types/lodash": "4.14.182",
    "@types/node": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "eslint": "8.20.0",
    "nodemon": "2.0.19",
    "typescript": "4.7.4"
  }
}
