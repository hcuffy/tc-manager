{
  "name": "tc-manager",
  "version": "0.1",
  "description": "Test Case Manager",
  "author": "Henry Cuffy",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "setup": "yarn install && \"cd backend\" yarn install",
    "dev": "REACT_APP_ENVIRONMENT=development ESLINT_NO_DEV_ERRORS=true react-scripts start",
    "dev:backend": "concurrently \"REACT_APP_ENVIRONMENT=development ESLINT_NO_DEV_ERRORS=true react-scripts start\" \"cd backend && yarn dev\"",
    "eject": "react-scripts eject",
    "fix": "yarn eslint src --fix",
    "kill": "fuser -k 3000/tcp",
    "lint": "yarn eslint src",
    "start": "react-scripts start",
    "start:prod": "node dist/server"
  },
  "dependencies": {
    "antd": "^4.21.6",
    "axios": "0.27.2",
    "connected-react-router": "^6.9.3",
    "history": "^5.3.0",
    "i18next": "21.8.5",
    "lodash": "4.17.21",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-i18next": "11.16.9",
    "react-redux": "8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "4.2.0",
    "redux-thunk": "2.4.1"
  },
  "devDependencies": {
    "@types/lodash": "4.14.182",
    "@types/node": "18.0.4",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "concurrently": "^7.3.0",
    "eslint": "8.16.0",
    "redux-logger": "3.0.6",
    "typescript": "4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
